    "joi": "^17.7.0",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.8.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.25.6",
    "swagger-ui-express": "^4.6.0"
  }
